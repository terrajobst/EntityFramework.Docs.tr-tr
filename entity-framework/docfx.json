{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**.png",
          "**.jpg"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata":
    {
      "breadcrumb_path": "/ef/breadcrumb/toc.json",
      "searchScope": ["Entity Framework"],
      "manager": "divega",
      "ms.topic": "article",
      "audience": "developer",
      "ms.reviewer": "divega",
      "ms.prod": "entity-framework",
      "ms.tgt_pltfrm": "dotnet",
      "ms.devlang": "dotnet"
    },
    "dest": "_site",
    "template": [
      "default"
    ]
  }
}
