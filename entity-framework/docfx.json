{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**.png",
          "**.jpg"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "breadcrumb_path": "/ef/breadcrumb/toc.json",
      "searchScope": [
        "Entity Framework"
      ],
      "bilingual_type": "hover over",
      "manager": "divega",
      "ms.topic": "conceptual",
      "audience": "developer",
      "ms.reviewer": "divega",
      "ms.prod": "entity-framework",
      "ms.tgt_pltfrm": "dotnet",
      "ms.devlang": "dotnet",
      "ms.author": "divega"
    },
    "fileMetadata": {
      "feedback_product_url": {
        "ef6/**/**.md": "https://github.com/aspnet/EntityFramework6/issues/new"
      },
      "ms.technology": {
        "core/**/**.md": "entity-framework-core",
        "ef6/**/**.md": "entity-framework-6",
        "efcore-and-ef6/**/**.md": "entity-framework-core"
      }      
    },
    "dest": "_site",
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig"
  }
}
